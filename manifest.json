{
    "name": "Parallel-Series calculator",
    "description": "Now you can 'extend' your time to things other than parallel and series calculations... see what I did there :)",
    "version": "0.1.0",
    "manifest_version": 3,
    "action":{
        "default_popup": "EquivalenceCalc.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": ["styles.css"]
      }
    ],
    "icons": [
        {
        "src": "images/icon96.png",
        "sizes": "96x96",
        "type": "image/png"
      }, {
        "src": "images/icon128.png",
        "sizes": "48x48",
        "type": "image/png"
      }, {
        "src": "images/icon144.png",
        "sizes": "144x144",
        "type": "image/png"
      }, {
        "src": "images/icon192.png",
        "sizes": "192x192",
        "type": "image/png"
      }]
}